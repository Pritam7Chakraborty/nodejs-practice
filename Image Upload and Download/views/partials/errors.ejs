<% if (errors && errors.length> 0) { %>
                <div class="bg-red-500 text-white p-4 rounded-md shadow-md mb-4">
                  <ul class="list-disc list-inside space-y-1">
                    <% errors.forEach(error=> { %>
                      <li>
                        <%= error %>
                      </li>
                      <% }) %>
                  </ul>
                </div>
                <% } %>
